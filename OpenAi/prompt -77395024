OpenAi response for prompt: ok write a c plus plus program using artificial intelligence to adding two number 
 ***************************



#include<iostream>
#include<math.h>

using namespace std;

class AI
{
    int num1, num2;
    int sum;
public:
    AI(){ num1 = 0; num2 = 0; }

    void input(){ cin >> num1 >> num2; }

    void sumAI(){
        sum = num1 + num2;
        cout << "The sum of the two number is: " << sum << endl;
    }
};

int main(){
    AI AIobj;
    AIobj.input();
    AIobj.sumAI();
    return 0;
}